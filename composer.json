{
    "name": "grachevko/symfony-skeleton",
    "license": "MIT",
    "type": "project",
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ],
        "files": [
            "src/Uuid/bootstrap.php"
        ],
        "exclude-from-classmap": ["**/Tests/"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7",
        "symfony/symfony": "^3",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.2",
        "stof/doctrine-extensions-bundle": "^1.2",
        "symfony/swiftmailer-bundle": "^2.3",
        "symfony/monolog-bundle": "^2.8",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "doctrine/doctrine-migrations-bundle": "^1",
        "guzzlehttp/guzzle": "^6",
        "ramsey/uuid-doctrine": "^1",
        "pagerfanta/pagerfanta": "^1"
    },
    "require-dev": {
        "sensio/generator-bundle": "^3.0",
        "doctrine/doctrine-fixtures-bundle": "^2.3",
        "incenteev/composer-parameter-handler": "^2.0",
        "symfony/phpunit-bridge": "^3.0",
        "hautelook/alice-bundle": "^1.3",
        "doctrine/data-fixtures": "^1.2",
        "phpunit/phpunit": "^5.5"
    },
    "scripts": {
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "build-parameters": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ]
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "hard",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml",
            "env-map": {
                "database_host": "database_host",
                "database_port": "database_port",
                "database_name": "database_name",
                "database_user": "database_user",
                "database_password": "database_password",
                "mailer_transport": "mailer_transport",
                "mailer_host": "mailer_host",
                "mailer_user": "mailer_user",
                "mailer_password": "mailer_password",
                "secret": "secret",
                "mail_delivery_address": "mail_delivery_address"
            }
        }
    }
}
